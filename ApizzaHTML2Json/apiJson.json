        "responses": {},
        "parameters": [
          {
            "name": "client_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "注册门户分配给应用的应用程序 ID"
          },
          {
            "name": "response_type",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "必须包括授权代码流的 code"
          },
          {
            "name": "redirect_uri",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "必须完全匹配你在应用注册门户中注册的 redirect_uris 之一，除了它必须采用 URL 编码"
          },
          {
            "name": "scope",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "由空格分隔的希望用户同意的 Microsoft Graph 权限列表"
          },
          {
            "name": "response_mode",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "指定用于将结果令牌发送回应用的方法，可以是 query 或 form_post"
          },
          {
            "name": "state",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "请求中包含的值将在令牌响应中返回"
          }
        ]
      }
    },
    "/{tenant}/oauth2/v2.0/token": {
      "parameters": [
        {
          "name": "tenant",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string"
          },
          "description": "允许的值为适用于 Microsoft 帐户和工作或学校帐户的 common、仅适用于工作或学校帐户的 organizations、仅适用于 Microsoft 帐户的 consumers 以及租户标识符（如租户 ID 或域名）"
        }
      ],
      "post": {
        "summary": "获取令牌",
        "tags": [
          "授权模块代表用户获取访问权限"
        ],
        "description": "",
        "responses": {
          "200": {
            "description": "请求成功",
            "content": {
              "application/json": {
                "examples": {
                  "请求成功范例": {
                    "value": {
                      "token_type": "Bearer",
                      "scope": "user.read Fmail.read",
                      "expires_in": 3600,
                      "access_token": "eyJ0eXAiOiJKV1QiLCJhbGciOiJSUzI1NiIsIng1dCI6Ik5HVEZ2ZEstZnl0aEV1Q...",
                      "refresh_token": "AwABAAAAvPM1KaPlrEqdFSBzjqfTGAMxZGUTdM0t4B4..."
                    }
                  }
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "client_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "注册门户分配给应用的应用程序 ID"
          },
          {
            "name": "grant_type",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "对于授权代码流必须为 authorization_code"
          },
          {
            "name": "scope",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "由空格分隔的希望用户同意的 Microsoft Graph 权限列表"
          },
          {
            "name": "code",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "你在流程的第一个图例中获得的 authorization_code"
          },
          {
            "name": "redirect_uri",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "用于获取 authorization_code 的相同的 redirect_uri 值"
          },
          {
            "name": "client_secret",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "你在应用注册门户中为应用创建的应用程序密码"
          }
        ]
      }
    },
