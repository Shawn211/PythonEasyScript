                  },
                  "preferredLanguage": {
                    "type": "string",
                    "description": "用户的首选语言。应遵循 ISO 639-1 代码；例如“EN-US”。"
                  },
                  "responsibilities": {
                    "type": "array",
                    "description": "供用户枚举其职责的列表。"
                  },
                  "schools": {
                    "type": "array",
                    "description": "供用户枚举其学习过的学校列表。"
                  },
                  "skills": {
                    "type": "array",
                    "description": "供用户枚举其技能的列表。"
                  },
