  "paths": {
    "//login.microsoftonline.com/{tenant}/adminconsent": {
      "parameters": [
        {
          "name": "tenant",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string"
          },
          "description": "允许的值为适用于 Microsoft 帐户和工作或学校帐户的 common、仅适用于工作或学校帐户的 organizations、仅适用于 Microsoft 帐户的 consumers 以及租户标识符（如租户 ID 或域名）"
        }
      ],
      "get": {
        "summary": "获取管理员同意",
        "tags": [
          "授权模块无用户访问"
        ],
        "description": "\nGET https://localhost/myapp/permissions?tenant=a8990e1f-ff32-408a-9f8e-78d3b9139b95&state=12345&admin_consent=True",
        "responses": {},
        "parameters": [
          {
            "name": "client_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "注册应用时由 Azure 应用注册门户分配的应用程序 ID"
          },
          {
            "name": "redirect_uri",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "必须完全匹配你在应用注册门户中注册的 redirect_uris 之一，除了它必须采用 URL 编码"
          },
          {
            "name": "state",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string"
            },
            "description": "请求中包含的值将在令牌响应中返回"
          }
        ]
      }
    },
    "//login.microsoftonline.com/{tenant}/oauth2/v2.0/token": {
      "parameters": [
        {
          "name": "tenant",
          "in": "path",
          "required": true,
          "schema": {
            "type": "string"
          },
          "description": "允许的值为适用于 Microsoft 帐户和工作或学校帐户的 common、仅适用于工作或学校帐户的 organizations、仅适用于 Microsoft 帐户的 consumers 以及租户标识符（如租户 ID 或域名）"
        }
      ],
      "post": {
        "summary": "获取访问令牌",
        "tags": [
          "授权模块无用户访问"
        ],
        "description": "在 OAuth 2.0 客户端凭据授予流中，可使用在注册应用时保存的应用程序 ID 和应用程序密码值直接从 Microsoft 标识平台 /token 终结点请求访问令牌。\n你可以通过传递 https://graph.microsoft.com/.default 并将其作为令牌请求中 scope 参数的值来指定预配置的权限。",
        "responses": {
          "200": {
            "description": "请求成功",
            "content": {
              "application/json": {
                "examples": {
                  "请求成功范例": {
                    "value": {
                      "token_type": "Bearer",
                      "expires_in": 3599,
                      "access_token": "eyJ0eXAiOiJKV1QiLCJhbGciOiJSUzI1NiIsIng1dCI6Ik1uQ19WWmNBVGZNNXBP..."
                    }
                  }
                }
              }
            }
          }
        },
        "parameters": [
          {
            "name": "client_id",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "注册应用时由 Azure 应用注册门户分配的应用程序 ID"
          },
          {
            "name": "scope",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "由空格分隔的希望用户同意的 Microsoft Graph 权限列表"
          },
          {
            "name": "client_secret",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "你在应用注册门户中为应用创建的应用程序密码"
          },
          {
            "name": "grant_type",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string"
            },
            "description": "必须是 client_credentials"
          }
        ]
      }
    },
    "//login.microsoftonline.com/{tenant}/oauth2/v2.0/authorize": {
