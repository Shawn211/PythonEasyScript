    "/users": {
      "post": {
        "summary": "创建用户",
        "tags": [
          "用户"
        ],
        "description": "创建新用户",
        "responses": {
          "200": {
            "description": "请求成功",
            "content": {
              "application/json": {
                "examples": {
                  "请求成功范例": {
                    "value": {
                      "@odata.context": "https://graph.microsoft.com/v1.0/$metadata#users/$entity",
                      "id": "id-value",
                      "businessPhones": [],
                      "displayName": "displayName-value",
                      "givenName": null,
                      "jobTitle": null,
                      "mail": null,
                      "mobilePhone": null,
                      "officeLocation": null,
                      "preferredLanguage": null,
                      "surname": null,
                      "userPrincipalName": "upn-value@tenant-value.onmicrosoft.com"
                    }
                  }
                }
              }
            }
          }
        },
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "accountEnabled": {
                    "type": "boolean",
                    "description": "启用此帐户时为 true，否则为 false"
                  },
                  "displayName": {
                    "type": "string",
                    "description": "要在用户的通讯簿中显示的名称"
                  },
                  "onPremisesImmutableId": {
                    "type": "string",
                    "description": "如果你对用户的 userPrincipalName (UPN) 属性使用联盟域，只需在创建新用户帐户时指定"
                  },
                  "mailNickname": {
                    "type": "string",
                    "description": "用户的邮件别名"
                  },
                  "passwordProfile": {
                    "type": "object",
                    "description": "用户的密码配置文件"
                  },
                  "userPrincipalName": {
                    "type": "string",
                    "description": "用户主体名称 (someuser@contoso.com)"
                  }
                },
                "required": [
                  "accountEnabled",
                  "displayName",
                  "onPremisesImmutableId",
                  "mailNickname",
                  "passwordProfile",
                  "userPrincipalName"
                ]
              }
            }
          }
        }
      },
